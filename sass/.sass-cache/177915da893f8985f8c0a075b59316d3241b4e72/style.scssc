@import 'reset';
@import 'variables';
@import 'mixins';

@font-face {
	font-family: 'icomoon';
	src:url('../fonts/icomoon.eot');
	src:url('../fonts/icomoon.eot?#iefix') format('embedded-opentype'),
		url('../fonts/icomoon.woff') format('woff'),
		url('../fonts/icomoon.ttf') format('truetype'),
		url('../fonts/icomoon.svg#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}

[data-icon]:before {
	font-family: 'icomoon';
	content: attr(data-icon);
	speak: none;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}


* {
	box-sizing:border-box;
	-moz-box-sizing:border-box;
	-webkit-box-sizing:border-box;
	-ms-box-sizing:border-box;
}


textarea, input, select { outline: none; }

.clear {
	clear:both;
}



.clear-fix {

	&:after {
		display:block;
		clear:both;
		content:"";
	}
}

img {
	width:100%;
}

a, a:visited {
	text-decoration:none;
	color:inherit;
}

html, body {
	font-family:$helvetica;	
	width:100%;
	height:100%;
	background:$white;
	font-size:15px;
	font-family: 'Droid Sans', sans-serif;
	line-height:1.2;
	color:$gray8;
	min-width:320px;
}

.header {

	position:relative;
	z-index:100;
	.dock &{
	
		@media all and (min-width: $bp-narrow){
			position:fixed;
			width:100%;
			background:$white;
			top:-80px;
			
			@include css-transition(top, .25s, 0, ease-out);
			
			&.show-user {
				top:0;
			}
		
		}
	}
}
	
.header-inner {
	margin:0 16px;

	
}

.header-content {
	max-width:$bp-narrow;
	margin:0 auto;
	padding:8px 0 0 0;
	
	
}

.user {
	position:relative;
}

.user-photo {
	float:left;
	width:64px;

	img {
		border-radius:100%;
	}
}

.user-info {
	float:left;
	padding:4px 12px ;
}

.user-name {
	font-size:28px;
	font-weight:bold;
	margin-bottom:4px;
}

.user-title {
	color:$gray5;
}

.user-contact {
	position:absolute;
	right:0;

}

.contact-item {
	height:28px;
	width:28px;
	float:left;
	display:block;
	
	margin:6px 0 0 12px;
	
	
	
	svg {
		fill:$gray3;
		
		&:hover {
			fill:$gray8;
		}
	}
	
	@media all and (min-width: $bp-narrow){
		margin:6px 0 0 16px;
	}
}

.nav {
	font-weight:bold;
	font-size:18px;
	padding:24px 0 8px 0;
	margin:0 16px;
	text-align:center;
	line-height:32px;
	border-bottom:1px solid $gray4;
	
	@include css-transition(all, .25s, 0, ease-out);

	a {
		color:$gray4;
		margin-left:6%;

		&:hover, &.active {
			color:$black;
		}
	}
	
	@media all and (min-width: $bp-narrow){
		font-size:20px;
		padding:16px 0 16px 0;
		
		a {
			margin-left:5.2%;
		}
	}
}

.content {
	width:100%;
	height:100%;
	
	.dock &{
	
		@media all and (min-width: $bp-narrow){
			margin-top:129px;
		}
	}
}

.section {
	padding:16px;
	max-width:$bp-regular2;
	margin:0 auto;
	margin-bottom:4%;
}

.section-head {
	font-weight:bold;
	font-size:36px;
	text-align:center;
	color:$gray4;
	padding:24px 0;
	max-width:$bp-regular;
	margin:0 auto;

	@include css-transition(all, .25s, 0, ease-out);
	
	span {
		white-space:nowrap;
	}

	@media all and (min-width: $bp-narrow){
		font-size:48px;
		padding:32px 0;

	}

	@media all and (min-width: $bp-regular){
		font-size:56px;
		padding:48px 0;
	}
}

.section-view {
	max-width:$bp-narrow2;
	margin:0 auto;
	position:relative;
}

.view-obj {
	border:2px solid $gray4;
	border-radius:6px;
	overflow:hidden;
	margin:0 auto;
	position:relative;
	background:$white;
	
	@include css-transition(all, .25s, 0, ease-out);
	
	width:100%;
		
	.view {
		padding-bottom:56.25%;
	}
	
	
	
	&.mobile {
		width:38%;
		.view {
			padding-bottom:150%;
			
		}
	}
	
	&.no-chrome {
		.view-chrome, .chrome-details{ 
			display:none;
		}
	}

	
	&.iphone {
		width:32.6%;
		border-radius:12px;
		padding:6% 1% 7% 1%;
		.view {
			padding-bottom:149%;
			border:1px solid $gray4;
			
		}
		
		.view-chrome{
			position:absolute;
			bottom:1.5%;
			left:50%;
			margin-left:-9%;
			width:18%;
			height:auto;
			padding-bottom:16%;
			border-radius:50%;
			border:1px solid $gray4;
			
			
			&:after {
				position:absolute;
				content:"";
				width:30%;
				height:30%;
				
				border:1px solid $gray4;
				left:50%;
				top:50%;
				@include translate3d(-50%, -50%, 0);
				
				
			}
			
			@media all and (min-width: $bp-mobile){
				width:17%;
			}
			
			
			
			@media all and (min-width: $bp-regular){
				width:16%;
				margin-left:-8%;
			}
		}
		
		
		.chrome-details {
			width:5.6%;
			height:auto;
			padding-bottom:6%;
			position:absolute;
			left:50%;
			margin-left:-2.8%;
			top:2%;
			&:before, &:after {
				opacity:0;
				
			}
		}
		
		@media all and (min-width: $bp-mobile){
			border-radius:18px;
		}
		
		@media all and (min-width: $bp-mobile2){
			border-radius:24px;
		}
		
		@media all and (min-width: $bp-regular){
			border-radius:32px;
		}
	}
	
	&.ipad {
		width:42%;
		border-radius:6px;
		padding:4% 2% 4% 2%;
		.view {
			padding-bottom:133.34%;
			border:1px solid $gray4;
			
		}
		
		.view-chrome{
			position:absolute;
			bottom:.5%;
			left:50%;
			margin-left:-4%;
			width:8%;
			height:auto;
			padding-bottom:8%;
			border-radius:50%;
			border:1px solid $gray4;
			
			
			&:after {
				position:absolute;
				content:"";
				width:30%;
				height:30%;
				
				border:1px solid $gray4;
				left:50%;
				top:50%;
				@include translate3d(-50%, -50%, 0);
				
				
			}
		}
		
		
		.chrome-details {
			width:1.8%;
			height:auto;
			padding-bottom:2%;
			position:absolute;
			left:50%;
			margin-left:-.9%;
			top:2%;
			&:before, &:after {
				opacity:0;
				
			}
		}
		
		@media all and (min-width: $bp-mobile){
			border-radius:9px;
		}
		
		@media all and (min-width: $bp-mobile2){
			border-radius:12px;
		}
		
		@media all and (min-width: $bp-regular){
			border-radius:16px;
		}
	}
	
	&.android {
		width:30%;
		border-radius:7px;
		padding:4% 1% 4% 1%;
		.view {
			padding-bottom:177.78%;
			border:1px solid $gray4;
			
		}
		
		.view-chrome{
			position:absolute;
			top:4%;
			height:92%;
			width:100%;
			left:0;
			background:$gray4;
		
		}
		
		
		.chrome-details {
			width:3.8%;
			height:auto;
			padding-bottom:4%;
			position:absolute;
			left:90%;
			top:1%;
			&:before, &:after {
				opacity:0;
				
			}
		}
		
		
		@media all and (min-width: $bp-mobile){
			border-radius:10px;
		}
		
		@media all and (min-width: $bp-mobile2){
			border-radius:14px;
		}
		
		@media all and (min-width: $bp-regular){
			border-radius:18px;
		}
	}
	
	&.small {
		
		&.standard {
			width:75%;
			margin:0;
			margin-bottom:6%;
			border-radius:4px;
			
			.view-chrome {
				height:12px;
			}
			
			.chrome-details {
					height:6px;
					width:6px;
					top:3px;
					left:3px;
					
					&:before, &:after {
						height:6px;
						width:6px;
					}
					
					&:before {
					
						left:10px;
					}
					
					&:after {
						
						left:20px;
					}
				}
		}
		
		&.ipad {
			position:absolute;
			width:33%;
			margin:0;
			right:0;
			top:6%;
			padding:3% 1.5% 3% 1.5%;
			border-radius:5px;
		
		}
		
		&.iphone {
			position:absolute;
			width:15%;
			margin:0;
			right:30%;
			top:26%;
			padding:2.5% .5% 3.2% .5%;
			border-radius:7px;
			border-width:1px;
			
		}
		
		@media all and (min-width: $bp-mobile){
		
			&.iphone {
				border-radius:8px;
				top:31%;
			}
			
			&.standard { 
				border-radius:6px;
				.view-chrome {
					height:16px;
				}
				
				.chrome-details {
					height:8px;
					width:8px;
					top:4px;
					left:4px;
					
					&:before, &:after {
						height:8px;
						width:8px;
					}
					
					&:before {
					
						left:14px;
					}
					
					&:after {
						
						left:28px;
					}
				}
				
				
			}
			
		
		}
		
		@media all and (min-width: $bp-mobile2){
			&.iphone {
				border-radius:12px;
				top:34%;
				border-width:2px;
			}
			
			&.ipad {
				border-radius:8px;
				
			}
			
			
		}
		
		
		@media all and (min-width: $bp-narrow){
			&.standard { 
				border-radius:6px;
				.view-chrome {
					height:24px;
				}
				
				.chrome-details {
					height:12px;
					width:12px;
					top:6px;
					left:6px;
					
					&:before, &:after {
						height:12px;
						width:12px;
					}
					
					&:before {
					
						left:20px;
					}
					
					&:after {
						
						left:40px;
					}
				}
			}
			
			&.iphone {
				
				top:35%;
				
			}
		
		}
		
		@media all and (min-width: $bp-regular){
			&.iphone {
				border-radius:16px;
				top:37%;
				
			}
			
			&.ipad {
				border-radius:12px;
				
			}
		}
	}
}

.view-nav {
	text-align:center;
	padding:16px 0;
	font-size:20px;
	font-weight:bold;
	color:$gray4;
	span {
		padding:0 12px;
		
		&:hover, &.active {
			color:$gray8;
		}
	}

	@media all and (min-width: $bp-narrow){
		font-size:24px;
		span {
			padding:0 16px;
		}
	}
}

.section-info {
	max-width:$bp-narrow;
	margin:0 auto;

	strong {
		font-weight:bold;
	}	
	
	em {
		font-style:italic;
	}
}

.info-inner {
	margin:0 16px;
	border-bottom:1px solid $gray4;
	
	padding:0 8px 8px 8px;
	
	font-size:13px;
	line-height:1.4;
	
	p {
		margin-bottom:16px;
	}
	
	@media all and (min-width: $bp-narrow){
		padding:8px 16px 16px 16px;
	}
}

.view-chrome {
	height:16px;
	background:$white;
	position:relative;
	border-bottom:1px solid $gray4;
	@include css-transition(all, .25s, 0, ease-out);
	
	@media all and (min-width: $bp-narrow){
		height:24px;
	}
}

.chrome-details {
	@include css-transition(all, .25s, 0, ease-out);
	height:8px;
	width:8px;
	border-radius:50%;
	background:$gray4;
	position:absolute;
	top:4px;
	left:4px;
	
	&:before, &:after {
		position:absolute;
		content:"";
		height:8px;
		width:8px;
		border-radius:50%;
		background:$gray4;
		@include css-transition(all, .25s, 0, ease-out);
		
	}
	
	&:before {
		top:0;
		left:12px;
	}
	
	&:after {
		top:0;
		left:24px;
	}
	
	@media all and (min-width: $bp-narrow){
		
		height:12px;
		width:12px;
		top:6px;
		left:6px;
		
		&:before, &:after {
			height:12px;
			width:12px;
		}
		
		&:before {
		
			left:20px;
		}
		
		&:after {
			
			left:40px;
		}
	}
}

.view {
	position:relative;
	@include css-transition(all, .25s, 0, ease-out);
	background:$white;
	
	
	
}

@include keyframes(showInfo) {
	0% {
		opacity:0;
	}
	100% {
		opacity:1;
	}
}


.view-content {
	width:100%;
	height:100%;
	overflow:auto;
	top:0;
	position:absolute;
	
	img {
		position:absolute;
		opacity:0;
	
		&:first-child {
			opacity:0;
		}
		
		&.hideImg {
			display:none;
		}
		
		&.showImg {
			opacity:1;
			@include css-animate(showInfo, .25s, 1, 0, ease-out);
		}
	}
	
}

.next-link {
	margin-top:40px;
	font-weight:bold;
	font-size:20px;
	color:$gray4;
	text-align:center;
	
	@include css-transition(font-size, .25s, 0, ease-out);
	
	.icon {
		width:36px;
		height:36px;
		fill:$gray4;
		vertical-align:-48%;
	}
	
	&:hover {
		color:$gray8;
		.icon {
			fill:$gray8;
		}
	}
	
	@media all and (min-width: $bp-narrow){
		margin-top:80px;
		font-size:36px;
		
		.icon {
			width:60px;
			height:60px;
			vertical-align:-44%;
		}
	}
	
}

.icon {
	display:inline-block;
}



.info-sec {
	display:none;
	
	&:first-child {
		display:block;
	}
	
	&.hideInfo {
		display:none;
	}
	
	&.showInfo {
		display:block;
		@include css-animate(showInfo, .5s, 1, 0, ease-out);
	}
