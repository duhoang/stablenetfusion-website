@include keyframes(hideSummary) {
	0% {
	 	@include translate3d(0, -50%, 0);	
		
	}
	100% {
		@include translate3d(0, 0, 0);	
	}	
} 

@include keyframes(showSummary) {
	0% {
	 	@include translate3d(0, 100%, 0);	
		
	}
	100% {
		@include translate3d(0, 0, 0);	
	}	
} 


.summary {
	z-index:100;
	max-width:768px;
	position:fixed;
	bottom:0;
	background:#ffeeb9;
	margin-left:72px;	
	box-shadow:2px -2px 4px rgba($black, .3);
	border-radius:0 44px 0 0;
	padding:20px 16px 16px 32px;
	font-size:16px;
	
	ul {
		list-style:disc outside;	
		margin:8px 16px;
	}
	
	li {
		margin-bottom:8px;	
	}
	
	@media only screen and (max-width : $bp-regular) {
			padding-left:24px ;
			margin-left:8px;
		}
	@media only screen and (max-width : $bp-narrow) {
		padding:16px;
		font-size:14px;
		
		bottom:44px;
	}
	
	@include css-animate(showSummary, .25s, 1,0, ease-out);
	
	
	&.hide-summary{
		@include css-animate(hideSummary, .25s, 1,0, ease-out);
	
	}
	
}

.summary-header {
	font-weight:bold;
	font-size:1.2em;
	padding-bottom:8px;
	
	span {
		display:none;
		
		.hide-summary & {
			display:inline;
			
		}
	}
	
	.hide-summary & {
			font-size:1em;
			position:relative;
			top:-6px;
		}
}

.rad-links {
	font-size:14px;
	span {
		color:$gray5;	
		font-weight:bold;
	}
	a {
		color:$blue-light;	
		margin:12px;
	}
	padding:8px 0 12px 0;
	
	@media only screen and (max-width : $bp-narrow) {
			
			display:none;
	}
}
.article-body {
	
	font-size:16px;
	padding:16px 8px 16px 36px;
	line-height:1.6;
	
	.comp-gadgets & {
		padding:16px 32px 16px 108px;
		
		@media only screen and (max-width : $bp-regular) {
			padding-left:24px ;
			
		}
	}
	p {
		margin-bottom:16px;
		
		strong {
			
			font-size:1.2em;	
		}
	}
	
	
	@media only screen and (max-width : $bp-narrow) {
		padding:16px;
		font-size:14px;
		
	}
}

/*******FRAMEWORK END********/

.button {
	float:left;
	color:$white;
	background:$red1;
	padding:4px 8px;
	box-shadow:0 1px 1px rgba($black, .5);	
	border-radius:1px;
	
	&.big {
		font-size:$font-big;
		height:36px;
		padding:0 24px;
		line-height:36px;
	}
}
.newsletter {
	background:$gray1;
	padding:8px;
	
	input {
		float:left;
		height:30px;
		line-height:30px;	
		font-size:13px;
		width:228px;
	}
