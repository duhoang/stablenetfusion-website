		font-size:20px;
		
		font-weight:normal;
	}
	@include shape;
	
	&:before {
		width:100%;
		height:100%;
		position:absolute;
		border-radius:100%;
		@include gradient-hori($blue-light, 0%, $purple, 100%);
		top:0;
		left:0;
	}
	
	&:after {
		width:58px;
		height:58px;
		position:absolute;
		border-radius:100%;
		background:$white;
		top:1px;
		left:1px;
	}
	
	span {
		position:relative;
		z-index:2;	
	}
	
	
	
	
}

.cpc-box {
	width:50%;
	max-width:320px;
	float:right;
	position:relative;
	left:16px;
	font-size:13px;
	line-height:1.4;
