.newsletter-header  {
	font-weight:bold;
	margin-bottom:4px;	
}

.article-feedback {
	margin:16px 8px;
	img {
		width:auto;
		vertical-align:-4px;
		margin-right:4px;
		position:relative;	
	}
	
	span {
		color:$blue-light;
		margin-right:16px;	
		font-weight:bold;
	}
}

.article-end {
	font-size:13px;
	
}
.comment-box {
