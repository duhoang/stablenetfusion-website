	max-width:$bp-wide;
	margin:0 auto;
	padding:0 8px;
	
	@media only screen and (max-width : $bp-narrow) {
		display:none;
	}
	
	&.dock-channel-nav {
		position:fixed;
		max-width:none;
		top:32px;
		left:0;
		width:100%;
		z-index:100;	
		padding:0;
		
		box-shadow:0 2px 2px rgba($black, .5);
	}
}
.item-type {
