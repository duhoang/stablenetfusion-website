@mixin keyframes($name) {
  @-webkit-keyframes #{$name} {
    @content; 
  }
  @-moz-keyframes #{$name} {
    @content;
  }
  @-ms-keyframes #{$name} {
    @content;
  }
  @keyframes #{$name} {
    @content;
  } 
}

@mixin css-animate($name, $time, $iteration, $delay, $ease) {
	animation: $name linear $time $iteration;
	-moz-animation: $name linear $time  $iteration;
	-webkit-animation: $name linear $time  $iteration;
	animation-delay: $delay;
	-moz-animation-delay: $delay; /* Firefox */
	-webkit-animation-delay: $delay; /* Safari and Chrome */
	-o-animation-delay: $delay; /* Opera */
	animation-timing-function:$ease;
	-moz-animation-timing-function:$ease; /* Firefox */
	-webkit-animation-timing-function:$ease; /* Safari and Chrome */
	-o-animation-timing-function:$ease; /* Opera */
}

@mixin image-2x($image, $width, $height) {
  @media (min--moz-device-pixel-ratio: 1.3),
         (-o-min-device-pixel-ratio: 2.6/2),
         (-webkit-min-device-pixel-ratio: 1.3),
         (min-device-pixel-ratio: 1.3),
         (min-resolution: 1.3dppx) {
    /* on retina, use image that's scaled by 2 */
    background-image: url($image);
    background-size: $width $height;
  }
}

@mixin gradient-vert($c-start, $start, $c-end, $end) {
	
	background: -moz-linear-gradient(top,  $c-start $start, $c-end $end); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop($start,$c-start), color-stop($end,$c-end)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  $c-start $start,$c-end $end); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  $c-start $start,$c-end $end); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  $c-start $start,$c-end $end); /* IE10+ */
	background: linear-gradient(to bottom,  $c-start $start,$c-end $end); /* W3C */
	-ms-filter: "progid:DXImageTransform.Microsoft.gradient(GradientType=0, startColorstr='#{ie-hex-str($c-start)}', endColorstr='#{ie-hex-str($c-end)}');";
	  filter: progid:DXImageTransform.Microsoft.gradient(GradientType=0, startColorstr='#{ie-hex-str($c-start)}', endColorstr='#{ie-hex-str($c-end)}');;
	  zoom: 1;

}
@mixin gradient-hori($c-start, $start, $c-end, $end){

background: -moz-radial-gradient(center, ellipse cover,   $c-start $start, $c-end $end); /* FF3.6+ */

background: -webkit-gradient(radial, center center, 0px, center center, 100%, color-stop($start,$c-start), color-stop($end,$c-end)); /* Chrome,Safari4+ */

background: -webkit-radial-gradient(center, ellipse cover,  $c-start $start,$c-end $end); /* Chrome10+,Safari5.1+ */

background: -o-radial-gradient(center, ellipse cover, $c-start $start,$c-end $end);  /* Opera 12+ */

background: -ms-radial-gradient(center, ellipse cover,  $c-start $start,$c-end $end);  /* IE10+ */

background: radial-gradient(ellipse at center,   $c-start $start,$c-end $end); /* W3C */

-ms-filter: "progid:DXImageTransform.Microsoft.gradient(GradientType=1, startColorstr='#{ie-hex-str($c-start)}', endColorstr='#{ie-hex-str($c-end)}');";
  filter: progid:DXImageTransform.Microsoft.gradient(GradientType=1, startColorstr='#{ie-hex-str($c-start)}', endColorstr='#{ie-hex-str($c-end)}');;
  zoom: 1;

}
	

@mixin ellipse-text {
	white-space:nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}

@mixin hardware-accel-style {
	-webkit-backface-visibility: hidden; 
	-moz-backface-visibility: hidden; 
	-ms-backface-visibility: hidden; 
	backface-visibility: hidden; 

}

@mixin translate3d($x, $y, $z) {
	-webkit-transform: translate3d($x, $y, $z); 
	-moz-transform: translate3d($x, $y, $z); 
	-ms-transform: translate3d($x, $y, $z); 
	transform: translate3d($x, $y, $z); 
}

@mixin scale($w, $h) {
	transform: scale($w, $h);
	-ms-transform: scale($w, $h); 
	-webkit-transform: scale($w, $h);
}


@mixin shape {
	position:relative;
	&:before,&:after{
		content: " ";
		height:0;
		width:0;
		position:absolute;
		pointer-events: none;
	}
}

@mixin rotate($deg){
	transform: rotate($deg);
	-webkit-transform: rotate($deg);
	-moz-transform: rotate($deg);
	-ms-transform: rotate($deg);
	-o-transform: rotate($deg);
}

@mixin rotate3d($x, $y, $z, $deg){
	transform: rotate3d($x, $y, $z, $deg);
	-webkit-transform: rotate3d($x, $y, $z, $deg);
	-moz-transform: rotate3d($x, $y, $z, $deg);
	-ms-transform:rotate3d($x, $y, $z, $deg);
	-o-transform: rotate3d($x, $y, $z, $deg);
	
}

@mixin translateRotateScale3d($x, $y, $z, $a, $b, $c, $deg, $scaleX, $scaleY) {
	-webkit-transform: translate3d($x, $y, $z) rotate3d($a, $b, $c, $deg) scale($scaleX, $scaleY);
	-moz-transform: translate3d($x, $y, $z) rotate3d($a, $b, $c, $deg) scale($scaleX, $scaleY);
	-ms-transform: translate3d($x, $y, $z) rotate3d($a, $b, $c, $deg) scale($scaleX, $scaleY);
	transform: translate3d($x, $y, $z) rotate3d($a, $b, $c, $deg) scale($scaleX, $scaleY);
	
}

@mixin preserve3d {
	-webkit-transform-style: preserve-3d;
	-moz-transform-style: preserve-3d;
	-ms-transform-style: preserve-3d;
	transform-style: preserve-3d;
	-webkit-backface-visibility: hidden;
	-moz-backface-visibility: hidden;
	-ms-backface-visibility: hidden;
	backface-visibility: hidden;
}

@mixin perspective($v, $x, $y) {
	perspective:$v;
	-webkit-perspective: $v;
	-ms-perspective:$v;
	-moz-perspective: $v;
	-webkit-perspective-origin: $x $y;
	-ms-perspective-origin:  $x $y;
	-moz-perspective-origin: $x $y;
	perspective-origin:  $x $y;
}


@font-face {
	font-family: 'icomoon';
	src:url('../fonts/icomoon.eot');
	src:url('../fonts/icomoon.eot?#iefix') format('embedded-opentype'),
		url('../fonts/icomoon.woff') format('woff'),
		url('../fonts/icomoon.ttf') format('truetype'),
		url('../fonts/icomoon.svg#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}


[data-icon]:before {
	font-family: 'icomoon';
	content: attr(data-icon);
	speak: none;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;
	-webkit-font-smoothing: antialiased;
}


html, body {
	width:100%;
	height:100%;
	font-size:$font-med;
	color:$text-black;
	font-family: $helvetica;
	line-height:1.2;
	
	min-width:320px;
	background:$white;
}

* {
	box-sizing:border-box;
	-moz-box-sizing:border-box;
	-webkit-box-sizing:border-box;
	-ms-box-sizing:border-box;
}

textarea, input, select { outline: none; }

.clear {
	clear:both;
}

a, a:hover, a:visited {
	color:inherit;
	text-decoration:none;
}


placeholder {
	color:$gray5;
} 
:-moz-placeholder, ::-moz-placeholder {
	color:$gray6;
}



.homepage-logo-wrap {
	max-width:420px;
	width:100%;
	padding:24px 0 0 0;
}	

.homepage-top {
	height:96px;
	
}

.icon-search {
	position:absolute;
	top:0;
	left:4px;
	width:44px;
	height:44px;
	background:url(../img/icon-search.png) no-repeat;
	
	.homepage-search:hover & {
		background-position:0 -44px;
	}
}



.homepage-search {
	max-width:640px;
	margin-top:20px;
	position:relative;
	border-bottom:3px solid $black;
	
	@include shape;
	
	&:before {
		width:3px; 
		height:8px;
		background:$black;
		bottom:0;
		left:0;
	}
	
	&:after {
		width:3px; 
		height:8px;
		background:$black;
		bottom:0;
		right:0;
	}
	
	input {
		font-size:$font-xbig;
		height:44px;
		display:block;
		width:100%;
		border:none;
		padding-left:52px;
	}
	
	&:hover {
		border-bottom-color:$red1;
		
		&:after, &:before {
			background:$red1;
		}
	}
}	

.must-read {
	width:320px;
	padding-right:8px;
}

.heading {
	padding:24px 0 8px;
	border-bottom:1px solid $gray4;
	font-size:24px;
}

.thumb {
	width:120px;
	height:88px;
	float:left;
}

.must-read-item {
	padding:8px 0;
	border-bottom:1px solid $gray4;
}

.title-link {
	padding: 4px 0 4px 12px;
	font-size:16px;
	color:$red1;
	float:left;
	width:180px;
}	

.content-info {
	position:absolute;
	bottom:8px;
	left:0;
	color:$black;
	background:rgba($white, 1);
	padding:8px 12px;
	font-size:13px;
	margin: 0 8px;
	
	.double & {
		width:auto;
	}
	
	.main-content & {
		width:auto;
		font-size:24px;
		padding:16px 32px;
		
	}
}

.content-title {
	font-size:18px;
	line-height:1;
	.main-content & {
		font-size:44px;
	}
}

.content-cat {
	position:absolute;
	color:$white;
	text-shadow:1px 1px 4px rgba($black, .8);
	right:8px;
	top:8px;
	font-size:13px;
}

.icon-play {
	width:120px;
	height:120px;
	position:absolute;
	top:16px;
	left:16px;
	background:url(../img/icon-play.png) no-repeat;
}


@include keyframes(showHeaderLogo) {
	0% {
		@include translate3d(-105px, 0, 0);
	}
	100% {
		@include translate3d(0, 0, 0);
	}
}


.logo-sm {
	width:105px;
	height:16px;
	background:url(../img/logo-about-sm.png) no-repeat;
	float:left;
	margin:12px 32px 16px 12px;
	display:none;
	
	.dock-header  &	{
		display:block;
		@include css-animate(showHeaderLogo, .25s, 1, 0, ease-out);
	}
	
	
}

.home {
	margin-left:12px;
	color:$red1;
}

.dock-header {
	.home {
		display:none;
	}
	
	
	
}


@include keyframes(showHeaderSearch) {
	0% {
		@include translate3d(320px, 0, 0);
	}
	100% {
		@include translate3d(0, 0, 0);
	}
}
.header-search {
	display:none;
	width:320px;
	position:relative;
	float:right;
	margin:8px 16px 0 0;
	border-bottom:2px solid $white;
	
	
	
	@include shape;
	
	&:before {
		width:2px; 
		height:4px;
		background:$white;
		bottom:0;
		left:0;
	}
	
	&:after {
		width:2px; 
		height:4px;
		background:$white;
		bottom:0;
		right:0;
	}
	
	input {
		font-size:$font-med;
		height:24px;
		display:block;
		width:100%;
		border:none;
		background:none;
		padding-left:36px;
		color:$white;
	}
	
	.dock-header &	 {
		display:block;
		@include css-animate(showHeaderSearch, .25s, 1, 0, ease-out);
	}
	
	
}	

.icon-search-sm {
	position:absolute;
	top:4px;
	left:8px;
	width:16px;
	height:16px;
	background:url(../img/icon-search-sm.png) no-repeat;
	
}

.icon-menu {
	position:absolute;
	top:12px;
	left:14px;
	width:16px;
	height:16px;
	background:url(../img/icon-menu.png) no-repeat;
	
}

.header-btn {
	width:44px;
	border-left:1px solid $gray7;
	height:44px;
	float:right;
	position:relative;
	display:none;
	
	.icon-search-sm {
		top:12px;
		left:14px;
	}
}

.header-nav {
	background:$black;
	color:$white;
	line-height:32px;
	font-size:14px;
	position:fixed;
	height:32px;
	left:0;
	top:0;
	width:100%;
	z-index:1000;
	
	span {
		margin-right:16px;
	}
	
	@media only screen and (max-width : 640px) {
		height:44px;
		span {
			display:none;
		}
		
		.header-btn {
			display:block;
		}
		
		.logo-sm 	{
			display:block;
			@include css-animate(showHeaderLogo, .25s, 1, 0, ease-out);
		}
		
		
	}
	
	
}



/*******FRAMEWORK********/
.header-dummy {
	height:44px;
}

.row {
	width:100%;
	margin: 0  auto;
	max-width:$breakp-wide;
	
	&:after {
		content:"";
		display:block;
		clear:both;
	}
}

.column {
	width:33.3%;
	float:left;
	word-break:keep-all ;
	
	&.double {
		width:66.6%;
	}
	
	@media only screen and (max-width : $breakp-reg) {
		width:50%;
		
		&:first-child {
			width:50%;
		}
		
		.left-rail &, .right-rail & {
			width:60%;
		}
		
		.left-rail &:last-child, .right-rail &:last-child {
			width:40%;
		}
		
		&.double {
			width:60%;
		}
	}
	
	@media only screen and (max-width : $breakp-mobile) {
		width:100%;
		
		&:first-child {
			width:100%;
		}
		
		.left-rail &:last-child, .right-rail &:last-child {
			width:100%;
		}
		
		&.double {
			width:100%;
		}
	}
}

img {
	width:100%;
}

.box {

	&.max-width-320 {
		max-width:320px;
		
		.left-rail & {
			float:right;
		}
		
		@media only screen and (max-width : $breakp-mobile) {
			max-width:none;
			.left-rail &  {
				float:none;
			}
		}
	}
	
	&.max-width-640 {
		max-width:792px;
	}
	
	
	
}

@include keyframes(itemFadeIn) {
	0% {
	
		opacity:0;
	
		@include translate3d(0, 44px, 0);

	}
	100% {
		opacity:1;
		@include translate3d(0, 0, 0);
	
	}
}

.item {
	position:relative;
	margin:8px;
	
	&.fadeIn-1 {
		@include css-animate(itemFadeIn, .5s, 1, 0, ease-out);
	}
	
	&.fadeIn-2 {
		@include css-animate(itemFadeIn, .35s, 1, 0, ease-out);
	}
	
	&.fadeIn-3 {
		@include css-animate(itemFadeIn, .75s, 1, 0, ease-out);
	}
}
	
.item-photo {
	&.fix-height {
		background-size:cover;
		background-position:center center;
		
		img {
			opacity:0;
		}
		
		@media only screen and (max-width : $breakp-mobile) {
			img {
				opacity:1;
			}	
			
			height:auto !important;
		}
	}
}

.item-title {
	background:$white;
	bottom:8px;
	left:8px;
	position:absolute;
	padding:8px;
	font-size:$font-big;
}

.left-rail {
	.double {
		float:right;
	}
}	

.half-column {
	width:50%;
	float:left;
	
	@media only screen and (max-width : $breakp-mobile) {
		width:100%;
		float:none;
	}
}

.list {
	padding:8px;
}

.list-header {
	padding:16px 0 4px 0;
	font-size:20px;
	border-bottom:2px solid $gray8;
}

.link-item {
	padding: 8px 0;
	border-bottom:1px solid $gray4;
	
	table {
		width:100%;
	}
	
	td {
		
		vertical-align:top;
		&.fix-width {
			width:1px;
		}
	}
}

.link-photo {
	img {
		width:120px;
	}
	
	
}

.link-title {
	padding:4px 4px 4px 12px;
	font-size:$font-big;
	
	a {
		color:$red1;
	}
}

.article-title {
	font-size:64px;
	padding:0 8px 16px 32px;
	line-height:1.1;
	color:$black;
	
	@media only screen and (max-width : $breakp-mobile) {
		padding:16px;
		font-size:$font-xxbig;
	}
}

.article-body {
	font-size:$font-big;
	padding:16px 8px 16px 32px;
	line-height:1.4;
	
	p {
		margin-bottom:16px;
	}
	
	@media only screen and (max-width : $breakp-mobile) {
		padding:16px;
		font-size:$font-med;
	}
}
/*******FRAMEWORK END********/


