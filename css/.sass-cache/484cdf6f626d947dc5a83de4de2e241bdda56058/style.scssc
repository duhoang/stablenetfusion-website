	font-weight:bold;
	font-size:$font-big;
}

.row {
	width:100%;
	margin: 0  auto;
	max-width:$bp-wide;
	
	&:after {
		content:"";
		display:block;
		clear:both;
	}
	
	&.left-rail, &.right-rail {
		position:relative;
	}
	
	&.hide-overflow {
		overflow-y:hidden;
		
		@include shape;
		
		&:before {
			width:320px;
			height:16px;
			right:0;
			bottom:0;
			
			@include gradient-vert(rgba($white,0), 0%, rgba($white,1), 100%);
			z-index:1;
		}
	}
}

.column {
	
	word-break:keep-all;
	
	@media only screen and (min-width : $bp-narrow) {
		
		width:50%;
		float:left;
		&.double {
			width:50%;	
		}
		
		&.main {
			width:100%;
			float:none;
			
			.left-rail & {
				padding-left:320px;
			}
			
			.right-rail & {
				padding-right:320px;
			}
			
		}
		
		&.rail {
			width:320px;
			position:absolute;
			top:0;
			
			.left-rail & {
				left:0;
			}
			
			.right-rail & {
				right:0;
			}
		}
	}
	
	@media only screen and (min-width : $bp-regular) {
		width:33.3%;
		float:left;
		
		&.double {
			width:66.6%;	
		}
	
	}

}

img {
