	}
}

@function strip-units($number) {
  @return $number / ($number * 0 + 1);
}

@mixin setPaddingMargin ($type, $val) {
	
	$list: ();
	@each $item in $val {
		$convert: strip-units($item) * 10 + 0px;
		$list: append($list, $convert);
	}
	#{$type}: $list;
	#{$type}: $val;
}

.item {
	position:relative;
	@include setPaddingMargin(margin, .8rem .8rem);
	
	.related-box & {
		margin:8px;	
	}
	
	&.fadeIn-1 {
		@include css-animate(itemFadeIn, .5s, 1, 0, ease-out);
	}
	
	&.fadeIn-2 {
