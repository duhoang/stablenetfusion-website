@include keyframes(hideSummary) {
	0% {
	 	@include translate3d(0, -50%, 0);	
		
	}
	100% {
		@include translate3d(0, 0, 0);	
	}	
} 

@include keyframes(showSummary) {
	0% {
	 	@include translate3d(0, 100%, 0);	
		
	}
	100% {
		@include translate3d(0, 0, 0);	
	}	
} 


.summary {
	z-index:100;
	max-width:768px;
	position:fixed;
	bottom:0;
	background:#ffeeb9;
	margin-left:16px;	
	box-shadow:2px -2px 4px rgba($black, .3);
	border-radius:0 44px 0 0;
	padding:20px 16px 16px 88px;
	font-size:16px;
	color:$blue1;
	ul {
		list-style:disc outside;	
		margin:8px 16px;
	}
	
	li {
		margin-bottom:8px;	
	}
	
	@media only screen and (max-width : $bp-regular) {
			padding-left:24px ;
			margin-left:8px;
		}
	@media only screen and (max-width : $bp-narrow) {
		padding:16px;
		font-size:14px;
		
		bottom:44px;
	}
	
	@include css-animate(showSummary, .25s, 1,0, ease-out);
	
	
	&.hide-summary{
		@include css-animate(hideSummary, .25s, 1,0, ease-out);
	
	}
	
}

.summary-header {
	font-weight:bold;
	font-size:1.2em;
	padding-bottom:8px;
	
	span {
		display:none;
		
		.hide-summary & {
			display:inline;
			
		}
	}
	
	.hide-summary & {
			font-size:1em;
			position:relative;
			top:-6px;
		}
}

.rad-links {
	font-size:14px;
	span {
		color:$gray5;	
		font-weight:bold;
	}
	a {
		color:$blue-light;	
		margin:12px;
	}
	padding:8px 0 12px 0;
	
	@media only screen and (max-width : $bp-narrow) {
			
			display:none;
	}
}
.article-body {
	
	font-size:16px;
	padding:16px 8px 16px 36px;
	line-height:1.6;
	
	.comp-gadgets & {
		padding:16px 32px 16px 108px;
		
		@media only screen and (max-width : $bp-regular) {
			padding-left:24px ;
			
		}
	}
	p {
		margin-bottom:16px;
		
		strong {
			
			font-size:1.2em;	
		}
	}
	
	
	@media only screen and (max-width : $bp-narrow) {
		padding:16px;
		font-size:14px;
		
	}
}

/*******FRAMEWORK END********/

.button {
	float:left;
	font-weight:bold;
	color:$white;
	background:$blue-light;
	padding:4px 8px;
	box-shadow:1px 1px 1px rgba($black, .5);	
	border-radius:1px;
}
.newsletter {
	background:$gray1;
	padding:8px;
	
	input {
		float:left;
		height:30px;
		line-height:30px;	
		font-size:13px;
		width:228px;
	}
}

.newsletter-header  {
	font-weight:bold;
	margin-bottom:4px;	
}

.article-feedback {
	margin:16px 8px;
	img {
		width:auto;
		vertical-align:-4px;
		margin-right:4px;
		position:relative;	
	}
	
	span {
		color:$blue-light;
		margin-right:16px;	
		font-weight:bold;
	}
}

.article-end {
	font-size:13px;
	
}
.comment-box {
	border-top:1px solid $gray3;
	margin:0 0 16px 104px;	
	max-width:632px;
	padding-top:16px;
	@media only screen and (max-width : $bp-narrow) {
		display:none;
	}
}
.channel-nav-outer {
	background:$white;
	max-width:$bp-wide;
	margin:0 auto;
	padding:0 8px;
	
	@media only screen and (max-width : $bp-narrow) {
		display:none;
	}
	
	&.dock-channel-nav {
		position:fixed;
		max-width:none;
		top:32px;
		left:0;
		width:100%;
		z-index:100;	
		padding:0;
		
		box-shadow:0 4px 4px rgba($black, .5);
	}
}
.item-type {
	position:absolute;
	top:8px;
	left:8px;
	img {
		width:auto;	
	}
}

.channel-nav {
	border-top:1px solid $gray3;
	font-family:$font-style-2;
	letter-spacing:1px;
	@include shape;
	&:before {
		height:2px;
		width:100%;
		bottom:-2px;
		left:0;
		@include gradient-hori($blue-light, 0%, $purple, 100%);
	}
	
	table {
		width:100%;
	}
	
	td {
		
		vertical-align:top;
		
		&.fix-width {
			white-space:nowrap;
		}
	}
	
	
}

.channel-nav-inner {
	.dock-channel-nav &{
		max-width:$bp-wide;
		margin:0 auto;
	}
}

.channel-nav-item {
	display:inline-block;
	font-size:16px;
	color:$blue-light;	
	line-height:44px;
	padding:0 16px 0 24px;
	@include shape;
	
	&:before {
		border:4px solid transparent;
		border-top:8px solid $blue-light;	
		top:50%;
		margin-top:-2px;
		left:8px;
	}
	
	/*&:after {
		height:12px;
		width:12px;
		border-radius:100%;
		border:1px solid $blue-light;
		top:50%;
		margin-top:-8px;
		left:6px;	
	}*/
}

.channel-nav-search {
	position:relative;
	border-left:1px solid $gray3;
	width:100%;
	input {
		font-size:16px;
		margin-top:6px;
		height:32px;
		line-height:32px;
		display:block;
		width:100%;
		border:none;
		padding-left:40px;
	}
}

.channel-nav-search-icon {
	width:20px;
	height:20px;
	background:url(../img/icon-search-blue.png);
	position:absolute;
	top:7px;
	left:10px;
}


.breadcrumb {
	color:$blue-light;
	font-size:14px;
	span {
		margin:0 8px;	
	}
	margin-bottom:8px;
	
	@media only screen and (max-width : $bp-narrow) {
	 padding-left:8px;	
	}
}
.top-area {
	max-width:$bp-wide;
	margin:0 auto;
	padding:0 8px;
	
	.main {
		padding-bottom:16px;
		
	}
	
	@media only screen and (max-width : $bp-narrow) {
		display:none;
	}
}


.top-logo {
	margin-top:16px;
	img {
		width:auto;	
	}
}

.top-ad {
	text-align:center;
	img {
		width:auto;	
	}
}

.top-search {
	max-width:640px;
	position:relative;
	width:100%;
	float:right;
	margin-right:8px;
	margin-top:20px;
	border-bottom:3px solid $black;
	
	@include shape;
	
	&:before {
		width:3px; 
		height:8px;
		background:$black;
		bottom:0;
		left:0;
	}
	
	&:after {
		width:3px; 
		height:8px;
		background:$black;
		bottom:0;
		right:0;
	}
	
	input {
		font-size:$font-xbig;
		height:44px;
		display:block;
		width:100%;
		border:none;
		padding-left:52px;
	}
	
	&:hover {
		border-bottom-color:$red1;
		
		&:after, &:before {
			background:$red1;
		}
	}
}	

.ad-tower {
	padding:8px 0;
	text-align:center;	
	img {
		width:auto;	
	}
}

.footer-nav {
	position:fixed;
	width:100%;
	height:52px;
	background:$black;
	bottom:0;
	left:0;
	z-index:10000;
	
	@media only screen and (min-width : $bp-narrow) {
		display:none;
	}
}

.footer-nav-item {
	height:52px;
	border-right:1px solid $gray8;
	width:25%;
	text-align:center;
	color:$gray5;
	font-size:$font-xsmall;
	float:left;
}

.footer-icon {
	height:32px;
	width:32px;
	border:2px solid $gray5;
	border-radius:100%;
	margin:4px auto;
}


.share {
	position:absolute;
	top:8px;
	left:12px;
	margin-bottom:12px;
	img {
		width:auto;	
		margin:0 0 0 14px;
	}
	
	float:left;
	@media only screen and (max-width : $bp-regular) {
		position:relative;
		left:0;
	}
	
	@media only screen and (max-width : $bp-narrow) {
			img {
			display:none;
		}	
		
		
	}
}

.date-box {
	height:60px;
	width:60px;
	border-radius:100%;
	text-align:center;
	
	margin:16px;	
	color:$blue-light;
	font-size:13px;
	padding:14px 4px;
	
	line-height:1;
	strong {
		font-size:20px;
		
		font-weight:normal;
	}
	@include shape;
	
	&:before {
		width:100%;
		height:100%;
		position:absolute;
		border-radius:100%;
		@include gradient-hori($blue-light, 0%, $purple, 100%);
		top:0;
		left:0;
	}
	
	&:after {
		width:58px;
		height:58px;
		position:absolute;
		border-radius:100%;
		background:$white;
		top:1px;
		left:1px;
	}
	
	span {
		position:relative;
		z-index:2;	
	}
	
	
	
	
}

.cpc-box {
	width:50%;
	max-width:320px;
	float:right;
	position:relative;
	left:16px;
	font-size:13px;
	line-height:1.4;
	h5 {
		color:$gray5;	
		font-size:14px;
	}
	
	a {
		display:block;
		
		&.t {
			color:$blue-light;
			font-size:14px;
			font-weight:bold;	
		}
		
		&.u {
			color:$gray5;
			text-decoration:underline;	
		}
	}
	
	@media only screen and (max-width : $bp-narrow) {
		
		float:none;
		border-top:1px solid $gray3;
		border-bottom:1px solid $gray3;
		
		width:auto;
		max-width:none;
		left:0;
		padding-top:8px;
		margin-bottom:8px;
		
	}
}

.related-link-item {
	padding:12px 8px;
	font-weight:bold;
	font-size:14px;
	color:$blue-light;	
}

.related-box {
	
	
	padding-top:12px;
	
	@include shape;
		
	&:before {
		height:1px;
		width:100%;
		top:0;
		left:0;
		@include gradient-hori($blue-light, 0%, $purple, 100%);
	}
	
	
	.list-header {
		padding:0 8px 8px 0;	
	}
	
	
	margin:0 8px;
	position:relative;
		width:auto;
		
		padding-bottom:32px;
		
		
		
	
	
	@media only screen and (min-width : $bp-narrow) {
		
		margin:0 16px;
		
		
	}
	
	@media only screen and (min-width : $bp-regular) {
		max-width:640px;
		margin-left:100px;
		
		
		
	}
	
	@media only screen and (min-width : 1344px) {
		position:absolute;
		width:240px;
		right:0;
		top:16px;
			
		.half-column {
			width:auto;	
		}
		
		.item {
			margin:0 0 8px 0;	
		}
		
	}
	
	
	
	
