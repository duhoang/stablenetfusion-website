.row {
	width:100%;
	margin: 0  auto;
	max-width:$bp-wide;
	
	&:after {
		content:"";
		display:block;
		clear:both;
	}
	
	&.left-rail, &.right-rail {
		position:relative;
	}
}

.column {
	
	word-break:keep-all;
	
	
	@media only screen and (min-width : $bp-narrow) {
		
		width:50%;
		float:left;
		&.main {
			width:100%;
			float:none;
			
			.left-rail & {
				padding-left:320px;
			}
			
			.right-rail & {
				padding-right:320px;
			}
			
		}
		
		&.rail {
			width:320px;
			position:absolute;
			top:0;
			
			.left-rail & {
				left:0;
			}
			
			.right-rail & {
				right:0;
			}
		}
	}
	
	@media only screen and (min-width : $bp-regular) {
		width:33.3%;
		float:left;
	}

}

