		padding:8px;
		border-bottom:1px dotted $gray3;
	/*	box-shadow:0 1px 2px rgba($black, .3);*/
		
		@include css-animate(dockTopArea, .25s, 1, 0, ease-out);
		
	}
}




.top-logo {
	margin:-1px 32px 0 8px;
	img {
		width:auto;	
	}
	
	.dock-toparea & {
		
		img {
			width:180px;
			margin-top:4px;
		
		}
	}
}

.top-ad {
	text-align:center;
	background:$gray1;
	display:none;
	img {
		width:auto;	
	}
	
	@media only screen and (min-width : $bp-narrow) {
		display:block;
	}
}

.side-ad {
	text-align:center;
	img {
		width:auto;	
	}
	
	
}

.main-left-rail {
	&.dock {
		position:fixed;
	}
}

.go-icon {
	width:32px;
	height:32px;
	pointer-events:none;
	background:url(../img/icons-sprite.png)-64px 0 no-repeat;
	float:right;
	position:relative;
	right:-8px;
	top:1px;
}

.top-search-icon {
	width:24px;
	height:24px;
	pointer-events:none;
	background:url(../img/icons-sprite.png) -4px -4px no-repeat;
	top:6px;
	left:6px;
	position:absolute;
	
}
.top-search {
	max-width:640px;
	position:relative;
	width:100%;
	float:right;
	margin:0 8px;
	border:1px solid $gray4;
	box-shadow:inset 0 1px 2px rgba($black, .3);
	
	
	/*
	@include shape;
	&:before {
		width:3px; 
		height:8px;
		background:$black;
		bottom:0;
		left:0;
	}
	
	&:after {
		width:3px; 
		height:8px;
		background:$black;
		bottom:0;
		right:0;
	}*/
	
	input {
		font-size:$font-big;
		height:36px;
		display:block;
		width:100%;
		border:none;
		padding-left:36px;
		background:none;
	}
	
}	


.footer-nav {
	position:fixed;
	width:100%;
	height:52px;
	background:$black;
	bottom:0;
	left:0;
	z-index:10000;
	
	@media only screen and (min-width : $bp-narrow) {
		display:none;
	}
}

.footer-nav-item {
	height:52px;
	border-right:1px solid $gray8;
	width:25%;
	text-align:center;
	color:$gray5;
	font-size:$font-xsmall;
	float:left;
}

.footer-icon {
	height:32px;
	width:32px;
	border:2px solid $gray5;
	border-radius:100%;
	margin:4px auto;
}


.hp-feature-head {
	padding:16px 8px 0 8px;
	font-size:20px;
	font-family: 'Roboto Slab', serif;
	margin-top:0;
	color:$blue-light;
}

.related-link-item {
	padding:8px 0;
}

.ad-tower {
	padding:8px;
	text-align:center;
	img {
		width:auto;
	}
}

.homepage-slideshow {
	@include shape;
	
&:before {
		border:16px solid transparent;
		border-top-color:$blue-light;
		bottom:-30px;
		left:86px;
		z-index:8;
	}	
}

.hp-slide-scroll {
	width:2000px;
}

.hp-slide-inner {
	max-width:1518px;
	margin:0 auto 8px auto;	
	background:$gray1;
	padding:0 44px;
	border-top:1px dotted $gray3;
	border-bottom:1px dotted $gray3;
	white-space:nowrap;
	overflow:hidden;
	height:68px;
	

	
	&:after {
		content:"";
		display:block;
		clear:both;
	}
	
	@include shape;
	
	&:before {
		height:100%;
		width:44px;
		position:absolute;
		left:0;
		top:0;
		z-index:2;
		@include gradient-hori(rgba($white,1), 0%, rgba($white,0), 50%);
	}
	
	
	&:after {
		height:100%;
		width:44px;
		position:absolute;
		right:0;
		top:0;
		z-index:2;
		@include gradient-hori(rgba($white,0), 0%, rgba($white,1), 50%);
	}
	
	@media only screen and (max-width : $bp-narrow) {
		
		margin:0;
		
		
	}
	
}

.hp-heading {
	@include shape;
	position:absolute;
	bottom:0;
	left:0;
	width:100%;
	padding:2px 0;
	/*text-shadow:1px 1px 2px rgba($black, 1);*/
	text-align:center;
	line-height:1.2;
	color:$white;
	white-space:normal;
	
	
	.active & {
		
	}
	
	span {
		position:relative;
		z-index:5;
	}
	
	
	
	&:before {
		width:100%;
		height:100%;
		/*background:rgba($white, .5);*/
		
		top:0;
		left:0;
		z-index:1;
	}
	
	&:after {
		width:100%;
		height:100%;
		/*background:rgba($black, .5);*/
		top:0;
		left:0;
		z-index:2;
		
		@include gradient-vert(rgba($black, 0), 0%, rgba($black, 1), 100%);
	}
	
	
}

.hp-slide-title {
	min-width:116px;
	height:68px;
	position:relative;
	float:left;
	display:block;
	font-size:12px;
	padding:8px 20px;
	margin-right:1px;
	font-weight:bold;
	line-height:0;
	color:$white;
	overflow:hidden;
	&.active, &:hover {
		background:$blue-light;
		color:$white;
		font-weight:bold;	
	}
}


.hp-slide-photo {
	width:88px;
	height:52px;
	
	position:relative;
	background-size:cover;
	background-position:center center;
	img {
		display:block;
	}	
}

.box {
	margin-bottom:1px;
	
	
	@media only screen and (min-width : $bp-narrow2) {
		width:50%;
		float:left;
		
		&.double {
			width:100%;
		}
		
		
		
	}
	
	@media only screen and (min-width : $bp-regular2) {
		width:33.3%;
		
		&.double {
			width:66.6%;
			
			
			/*&:nth-child(even) {
				float:right;
				margin-bottom:8px;	
			}*/
		}
	}	
}

.top-area-table {
	width:100%;
	
	.fix-width {
		width:1px;
	}
	
	td {
		vertical-align:top;
	}
}

.hp-sec-left {
	width:32px;
	height:32px;
	background:url(../img/icons-sprite.png) -32px 0 no-repeat;
	position:absolute;
	top:50%;
	left:4px;
	margin-top:-16px;
	z-index:10;
	background-color:rgba($white, .5);
	border-radius:100%;
}

.hp-sec-right {
	width:32px;
	height:32px;
	background:url(../img/icons-sprite.png) -32px -32px no-repeat;
	position:absolute;
	top:50%;
	right:4px;
	z-index:4;
	margin-top:-16px;
	z-index:10;
	
	background-color:rgba($white, .5);
	border-radius:100%;
}

.hp-editor-label {
	font-size:20px;
	white-space:normal;
	line-height:1.2;
	padding-top:4px;
}

.homepage-section {
	
	margin-top:16px;
	max-width:$carousel;
	margin:16px auto;
}

.hp-sec-heading {
	
	border-top:1px dotted $gray3;
	max-width:$bp-wide;
	padding:16px;
	font-size:24px;
	font-family: $robotoslab;
	max-width:$bp-wide;
	margin:0 auto;
	color:$blue-light;
	
}

.share-block {
	font-size:14px;
	color:$blue-light;
	bottom:16px;
	right:16px;
	position:absolute;
		
	.double & {
		
		right:32px;
	}
	
	.tweet & {
		color:$white;
	}
}
.share-icon {
	width:24px;
	height:24px;
	background:url(../img/icons-sprite.png) -100px -4px no-repeat;
	display:inline-block;
	vertical-align:-6px;
	margin-right:4px;
	
}

.see-more {
	float:right;
	font-family:$helvetica;
	font-size:16px;
	color:$blue-light;
}
.see-more-icon {
	width:32px;
	height:32px;
	background:url(../img/icons-sprite.png) -64px -32px no-repeat;
	display:inline-block;
	vertical-align:-10px;
	margin-right:4px;
	
}

.content-title-bg {
	padding:16px;
	
	@media only screen and (min-width : $bp-regular) {
		padding-left:36px;
		
	}
}

.content-title {
	font-size:$font-xxbig;
	@media only screen and (min-width : $bp-narrow) {
		font-size:$font-xxxbig;
	}
	
	
	
}

.content-subtitle {
	font-size:$font-big;
	@media only screen and (min-width : $bp-narrow) {
		font-size:$font-xbig;
	}
}

.content-related-column {
	
	padding:16px;
	font-size:$font-med;
	
	@media only screen and (min-width : $bp-narrow) {
		
		.half-column {
			width:auto;
			float:none;
		}
	}
	
	@media only screen and (min-width :  $bp-narrow1) {
		
		.half-column {
			width:50%;
			float:left;
		}
	}
	
	@media only screen and (min-width : $bp-regular) {
		position:absolute; 
		right:8px;
		top:8px;
		width:240px;
		padding:0;
		
		.half-column {
			width:auto;
			float:none;
		}
	}
}	

.content-body {
	position:relative;
	
}

.content-body-inner {
	padding:16px;
	font-size:$font-med;
	
	@media only screen and (max-width : $bp-regular) {
		&[style] {
			min-height: inherit !important;
		}
		
	}
	
	
	
	@media only screen and (min-width : $bp-narrow) {
		font-size:$font-big;
		
		
	}
	
	@media only screen and (min-width : $bp-regular) {
		
		padding-right:260px;
	}
	
	
	@media only screen and (min-width : $bp-regular2) {
		padding-left:36px;
	}
}


.newsletter-sub-fields {

	input {
		display:block;
		width:100%;
		max-width:320px;
		float:left;
	}
}

.outline {
	border:1px dashed $gray4;
	padding:8px;
}

.related-box {
	li {
		list-style:disc outside;
		margin-left:16px;
	}
}

.middle-area {
	position:relative;
	padding-top:44px;
	@media only screen and (min-width : $bp-narrow) {
		padding-top:0;
		
		.dock-toparea & {
			padding-top:120px;
		}
	}
}

.bottom-area {
	position:relative;
	z-index:1;
	background:$gray1;
}

.share-module {
	background:rgba($white, .5);
	width:48px;
	position:fixed;
	right:0;
	z-index:10;
	
}

.top-share {
	padding:0 8px;
	float:right;
	position:relative;
	top:-4px;
}


.top-share-head {
	font-size:$font-small;
	font-weight:bold;
	color:$gray8;
}

.top-share-icon {
	width:32px;
	height:32px;
	float:left;
	margin-right:8px;
	background:url(../img/icons-sprite.png) -128px 0;
	
	&.facebook {
		background-position:-160px 0;
	}
	
	&.stumbleupon {
		background-position:-224px 0;
	}
	
	&.googleplus {
		background-position:-192px 0;
	}
	
}

.hp-sec-carousel {
	position:relative;
	@include shape;
	overflow:hidden;
	
	
	&:before {
		height:100%;
		width:44px;
		position:absolute;
		left:0;
		top:0;
		z-index:2;
		@include gradient-hori(rgba($white,1), 0%, rgba($white,0), 50%);
	}
	
	
	&:after {
		height:100%;
		width:44px;
		position:absolute;
		right:0;
		top:0;
		z-index:2;
		@include gradient-hori(rgba($white,0), 50%, rgba($white,1), 100%);
	}
	
}

.hp-sec-car-inner {
	margin:0 auto;
	max-width:$bp-wide;
	white-space:nowrap;
	position:relative;
}

.hp-sec-box {
	white-space:normal;
	width:50%;
	min-width:192px;
	padding:0 8px;
	
	display:inline-block;
	vertical-align:top;
	
	.item-info-inner{
		padding:8px 8px 0 8px;
	}
	
	@media only screen and (min-width : $bp-mobile2) {
		width:33.3%;
	}
	
	@media only screen and (min-width : $bp-regular) {
		padding:0 16px 0 8px;
		width:25%;
		
	}
	
}

.slide-item-blurb {
	font-size:$font-small;
	
	@media only screen and (min-width : $bp-regular) {
		font-size:$font-med;
	}
}

.slide-item-photo {
	position:absolute;
}
.slide-item-title {
	font-size:$font-med;
	font-family:$robotoslab;
	line-height:1.1;
	padding:8px;
	background:$white;
	position:absolute;
	bottom:0;
	
	@media only screen and (min-width : $bp-regular) {
		font-size:$font-big;
		
	}
	
	@media only screen and (min-width : $bp-regular2) {
		font-size:$font-xbig;
		
	}
}

.slide-item-info {
	padding:8px 4px;
	
	@media only screen and (min-width : $bp-regular) {
		padding:8px;
	}
	
	.share-block {
		position:relative;
		right:auto;
		bottom:auto;
		margin-top:8px;
	}
}

.homepage-feature {

	@media only screen and (min-width : $bp-narrow) {
		padding-left:8px;
	
	}
}

.hp-sec-links {
	max-width:$bp-wide;
	margin:8px auto;
	font-size:$font-small;
	color:$blue-light;
	ul {
		padding:0  16px;
	}
	li {
		display:inline-block;
		/*background:$gray4;*/
		color:$blue-light;
		padding:4px 6px 2px 4px;
		
		margin-bottom:1px;
		
		
	}
	
	
}	


.ad-native {
	width:300px;
	position:relative;
	margin:8px auto;
}

.hp-sec-ad {
	display:inline-block;
	width:25%;
	min-width:300px;
	position:relative;
	padding-top:8px;
	
	
}

.hp-sec-ad-inner {
	width:300px;
	margin:0 auto;
	img {
		width:auto;
	}
}

.item-top-wrap {
	position:relative;
}

.browse-topics {
	float:right;
	font-size:20px;
	color:$gray4;
}

.hp-sec-links-col {
	width:180px;
	display:inline-block;
	padding-left:16px;
	vertical-align:top;
}	

.letter-block {
	background:$gray4;
	border-radius:100%;
	width:56px;
