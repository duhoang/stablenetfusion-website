		}
		
		&.main {
			width:100%;
			float:none;
			
			.left-rail & {
				padding-left:320px;
			}
			
			.right-rail & {
				padding-right:320px;
			}
			
		}
		
		&.rail {
			width:320px;
			position:absolute;
			top:0;
			
			.left-rail & {
				left:0;
			}
			
			.right-rail & {
				right:0;
			}
		}
	}
	
	@media only screen and (min-width : $bp-regular) {
		width:33.3%;
		float:left;
		
		&.double {
			width:66.6%;	
		}
	
	}

