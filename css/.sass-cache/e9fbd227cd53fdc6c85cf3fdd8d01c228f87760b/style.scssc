.row {
	width:100%;
	margin: 0  auto;
	max-width:$bp-wide;
	
	&:after {
		content:"";
		display:block;
		clear:both;
	}
	
	&.left-rail, &.right-rail {
		position:relative;
	}
	
	&.hide-overflow {
		overflow-y:hidden;
		
		@include shape;
		
		&:before {
			width:320px;
			height:16px;
			right:0;
			bottom:0;
			
			@include gradient-vert(rgba($white,0), 0%, rgba($white,1), 100%);
			z-index:1;
		}
