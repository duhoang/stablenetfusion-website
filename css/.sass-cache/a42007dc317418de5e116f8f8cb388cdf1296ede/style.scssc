@function strip-units($number) {
  @return $number / ($number * 0 + 1);
}

@mixin setPaddingMargin ($type, $val) {
	
	$list: ();
	@each $item in $val {
		$convert: strip-units($item) * 10 + 0px;
		$list: append($list, $convert);
	}
	#{$type}: $list;
	#{$type}: $val;
}

