	padding:14px 4px;
	
	line-height:1;
	strong {
		font-size:20px;
		
		font-weight:normal;
	}
	@include shape;
	
	&:before {
		width:100%;
		height:100%;
		position:absolute;
		border-radius:100%;
		@include gradient-hori($blue-light, 0%, $purple, 100%);
		top:0;
		left:0;
	}
	
	&:after {
		width:58px;
		height:58px;
		position:absolute;
		border-radius:100%;
		background:$white;
		top:1px;
		left:1px;
	}
	
	span {
		position:relative;
		z-index:2;	
	}
	
	
	
	
}

.cpc-box {
	width:50%;
	max-width:320px;
	float:right;
	position:relative;
	left:16px;
	font-size:13px;
	line-height:1.4;
	h5 {
		color:$gray5;	
		font-size:14px;
	}
	
	a {
		display:block;
		
		&.t {
			color:$blue-light;
			font-size:14px;
			font-weight:bold;	
		}
		
		&.u {
			color:$gray5;
			text-decoration:underline;	
		}
	}
	
	@media only screen and (max-width : $bp-narrow) {
		
		float:none;
		border-top:1px solid $gray3;
		border-bottom:1px solid $gray3;
		
