	margin:16px auto 8px auto;	
	padding:0 44px;
	border-top:1px dotted $gray3;
	border-bottom:1px dotted $gray3;
	white-space:nowrap;
	overflow:hidden;
	height:36px;
	
	&:after {
		content:"";
		display:block;
		clear:both;
	}
	
	@include shape;
	
	&:before {
		height:100%;
		width:44px;
		position:absolute;
		left:0;
		top:0;
		@include gradient-hori(rgba($white,1), 0%, rgba($white,0), 50%);
	}
	
	
	&:after {
		height:100%;
		width:44px;
		position:absolute;
		right:0;
		top:0;
		@include gradient-hori(rgba($white,0), 0%, rgba($white,1), 50%);
	}
	
	@media only screen and (max-width : $bp-narrow) {
		
		margin:0;
		
		
	}
	
