background: -webkit-gradient(linear, left bottom, right top, color-stop(50%,#ffeeb9), color-stop(100%,#d0d0d0));

background: -webkit-linear-gradient(45deg,  #ffeeb9 50%,#d0d0d0 100%);

background: -o-linear-gradient(45deg,  #ffeeb9 50%,#d0d0d0 100%);

background: -ms-linear-gradient(45deg,  #ffeeb9 50%,#d0d0d0 100%);

background: linear-gradient(45deg,  #ffeeb9 50%,#d0d0d0 100%);

filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffeeb9', endColorstr='#d0d0d0',GradientType=1 );


	}
}

@include keyframes(hideSummary) {
	0% {
	 	@include translate3d(0, -50%, 0);	
		
	}
	100% {
		@include translate3d(0, 0, 0);	
	}	
} 

@include keyframes(showSummary) {
	0% {
	 	@include translate3d(0, 100%, 0);	
		
	}
	100% {
		@include translate3d(0, 0, 0);	
	}	
} 


.summary {
	z-index:100;
	max-width:768px;
	position:fixed;
	bottom:0;
	background:#ffeeb9;
	margin-left:72px;	
	box-shadow:2px -2px 4px rgba($black, .3);
	border-radius:0 44px 0 0;
	padding:20px 16px 16px 32px;
	font-size:16px;
	
	ul {
		list-style:disc outside;	
		margin:8px 16px;
	}
	
	li {
		margin-bottom:8px;	
	}
	
	@media only screen and (max-width : $bp-regular) {
			padding-left:24px ;
			margin-left:8px;
		}
	@media only screen and (max-width : $bp-narrow) {
		padding:16px;
		font-size:14px;
		
		bottom:44px;
	}
	
	@include css-animate(showSummary, .25s, 1,0, ease-out);
	
	
	&.hide-summary{
		@include css-animate(hideSummary, .25s, 1,0, ease-out);
	
	}
	
}

.summary-header {
	font-weight:bold;
	font-size:1.2em;
	padding-bottom:8px;
	
	span {
		display:none;
		
		.hide-summary & {
			display:inline;
			
		}
	}
	
	.hide-summary & {
			font-size:1em;
			position:relative;
			top:-6px;
		}
}

.rad-links {
	font-size:14px;
	span {
		color:$gray5;	
		font-weight:bold;
	}
	a {
		color:$blue-light;	
		margin:12px;
	}
	padding:8px 0 12px 0;
	
	@media only screen and (max-width : $bp-narrow) {
			
			display:none;
	}
}
.article-body {
	
	font-size:16px;
	padding:16px 8px 16px 36px;
	line-height:1.6;
	
	.comp-gadgets & {
		padding:16px 32px 16px 108px;
		
		@media only screen and (max-width : $bp-regular) {
			padding-left:24px ;
			
		}
	}
	p {
		margin-bottom:16px;
		
		strong {
			
			font-size:1.2em;	
		}
	}
	
	
	@media only screen and (max-width : $bp-narrow) {
		padding:16px;
		font-size:14px;
		
	}
}

/*******FRAMEWORK END********/

.button {
	float:left;
	color:$white;
	background:$red1;
	padding:4px 8px;
	box-shadow:0 1px 1px rgba($black, .5);	
	border-radius:1px;
	
	&.big {
		font-size:$font-big;
		height:36px;
		padding:0 24px;
		line-height:36px;
	}
}
.newsletter {
	background:$gray1;
	padding:8px;
	
	input {
		float:left;
		height:30px;
		line-height:30px;	
		font-size:13px;
		width:228px;
	}
}

.newsletter-header  {
	font-weight:bold;
	margin-bottom:4px;	
}

.article-feedback {
	margin:16px 8px;
	img {
		width:auto;
		vertical-align:-4px;
		margin-right:4px;
		position:relative;	
	}
	
	span {
		color:$blue-light;
		margin-right:16px;	
		font-weight:bold;
	}
}

.article-end {
	font-size:13px;
	
}
.comment-box {
	border-top:1px solid $gray3;
	margin:0 0 16px 104px;	
	max-width:632px;
	padding-top:16px;
	@media only screen and (max-width : $bp-narrow) {
		display:none;
	}
}
.channel-nav-outer {
	background:$white;
	max-width:$bp-wide;
	margin:0 auto;
	padding:0 8px;
	
	@media only screen and (max-width : $bp-narrow) {
		display:none;
	}
	
	&.dock-channel-nav {
		position:fixed;
		max-width:none;
		top:32px;
		left:0;
		width:100%;
		z-index:100;	
		padding:0;
		
		box-shadow:0 2px 2px rgba($black, .5);
	}
}
.item-type {
	position:absolute;
	top:8px;
	left:8px;
	img {
		width:auto;	
	}
}

.channel-nav {
	font-family:$font-style-1;
	@include shape;
	&:before {
		height:1px;
		width:100%;
		top:-1px;
		left:0;
		@include gradient-hori($blue-light, 0%, $purple, 100%);
	}
	
	&:after {
		height:1px;
		width:100%;
		bottom:-1px;
		left:0;
		@include gradient-hori($blue-light, 0%, $purple, 100%);
	}
	
	table {
		width:100%;
	}
	
	td {
		
		vertical-align:top;
		
		&.fix-width {
			white-space:nowrap;
		}
	}
	
	
}

.channel-nav-inner {
	.dock-channel-nav &{
		max-width:$bp-wide;
		margin:0 auto;
	}
}

.channel-nav-item {
	display:inline-block;
	font-size:16px;
	color:$blue-light;	
	line-height:44px;
	padding:0 16px 0 24px;
	@include shape;
	
	&:before {
		border:4px solid transparent;
		border-top:8px solid $blue-light;	
		top:50%;
		margin-top:-2px;
		left:8px;
	}
	
	/*&:after {
		height:12px;
		width:12px;
		border-radius:100%;
		border:1px solid $blue-light;
		top:50%;
		margin-top:-8px;
		left:6px;	
	}*/
}

.channel-nav-search {
	position:relative;
	/*border-left:1px solid $gray3;*/
	width:100%;
	input {
		font-size:16px;
		margin-top:6px;
		height:32px;
		line-height:32px;
		display:block;
		width:100%;
		border:none;
		padding-left:40px;
	}
}

.channel-nav-search-icon {
	width:20px;
	height:20px;
	background:url(../img/icon-search-blue.png);
	position:absolute;
	top:7px;
	left:10px;
}


.breadcrumb {
	color:$blue-light;
	font-size:14px;
	span {
		margin:0 8px;	
	}
	margin-bottom:8px;
	
	@media only screen and (max-width : $bp-narrow) {
	 padding-left:8px;	
	}
}

.top-area {
	max-width:$bp-wide;
	margin:0 auto;
	padding:12px 8px;
	
	@media only screen and (max-width : $bp-narrow) {
		display:none;
	}
}


.top-logo {
	margin:0 16px 0 8px;
	img {
		width:auto;	
	}
}

.top-ad {
	text-align:center;
	img {
		width:auto;	
	}
}

.go-icon {
	width:32px;
	height:32px;
	pointer-events:none;
	background:url(../img/icons-sprite.png)-64px 0 no-repeat;
	float:right;
	position:relative;
	right:-8px;
	top:1px;
}

.top-search-icon {
	width:24px;
	height:24px;
	pointer-events:none;
	background:url(../img/icons-sprite.png) -4px -4px no-repeat;
	top:6px;
	left:6px;
	position:absolute;
	
}
.top-search {
	max-width:640px;
	position:relative;
	width:100%;
	float:right;
	margin:0 8px;
	border:1px solid $gray4;
	box-shadow:inset 0 1px 2px rgba($black, .3);
	
	
